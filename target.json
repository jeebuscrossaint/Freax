{
  "llvm-target": "riscv64-unknown-unknown-elf",
  "data-layout": "e-m:e-p:64:64-i64:64-i128:128-n64-S128",
  "arch": "riscv64",
  "os": "none",
  "vendor": "unknown",
  "linker-flavor": "ld.lld",
  "executables": true,
  "linker": "rust-lld",
  "pre-link-args": {
    "ld.lld": [
      "--script=linker.ld"
    ]
  },
  "features": "+m,+a,+c",
  "panic-strategy": "abort",
  "disable-redzone": true,
  "relocation-model": "static",
  "code-model": "kernel",
  "eliminate-frame-pointer": false
}